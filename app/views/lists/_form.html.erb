<%= form_with(model: [@board, @list], local: true, class: "ui form") do |f|%>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name%>
    </div>
    <div>
        <%= f.label :priority %>
        <%= f.select :priority, options: ["High", "Medium", "Low"] %>
    </div>
    <div>
        <%= f.submit %>
    </div>
<% end %>
